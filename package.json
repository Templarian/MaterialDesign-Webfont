{
  "name": "@andes/font",
  "version": "0.5.0",
  "description": "Andes WebFont for any helth application",
  "style": "css/andes-font.css",
  "scripts": {
    "build": "npm run css && npm run css-min",
    "css": "node-sass --source-map true --include-path scss scss/andes-font.scss css/andes-font.css",
    "css-min": "node-sass --source-map true --include-path scss scss/andes-font.scss css/andes-font.min.css --output-style compressed"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/liquid36/andes-font.git"
  },
  "keywords": [
    "andes",
    "design",
    "icons",
    "webfont"
  ],
  "author": {
    "name": "Mariano Botta"
  },
  "licenses": [
    {
      "type": "OFL-1.1",
      "url": "http://scripts.sil.org/OFL"
    },
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/mit-license.html"
    }
  ],
  "bugs": {
    "url": "https://github.com/liquid36/andes-font/issues"
  },
  "homepage": "https://www.andes.gob.ar",
  "devDependencies": {
    "node-sass": "^4.9.0"
  }
}
